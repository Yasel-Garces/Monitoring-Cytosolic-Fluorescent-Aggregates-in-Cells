############# Some functions ###############
statisticBYcolumns<-function(data){
dataMean<-c()
dataVar<-c()
dataSD<-c()
for(i in 1:dim(data)[2]){
dataMean[i]<-mean(data[,i])
dataVar[i]<-var(data[,i])
dataSD[i]<-sd(data[,i])
}
return(list(Mean=dataMean,Var=dataVar,Sd=dataSD))
}
